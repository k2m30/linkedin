<%= form_tag import_path, multipart: true do %>
    <p>
      <%= label_tag :file %>
      <%= file_field_tag :file %></p>
    <p>
      <%= label_tag :owner %>
      <%= text_field_tag :owner %></p>
    <p>
      <%= label_tag :passed_to %>
      <%= text_field_tag :passed_to %></p>
    <p><%= submit_tag 'Import' %></p>
<% end %>